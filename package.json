{
  "version": "1.1.0",
  "license": "MIT",
  "type": "module",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsup --watch",
    "build": "tsup",
    "test": "true",
    "lint": "true",
    "prepare": "yarn build"
  },
  "_husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "remark-typescript-tools",
  "author": "Lenz Weber",
  "module": "dist/index.js",
  "devDependencies": {
    "@types/node": "^22.5.0",
    "@types/react": "^18.3.4",
    "husky": "^9.1.4",
    "release-it": "^17.6.0",
    "remark-mdx": "^3.0.1",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "tslib": "^2.6.3",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "unified": "^11.0.5"
  },
  "dependencies": {
    "@microsoft/tsdoc": "^0.15.0",
    "@types/estree": "^1.0.5",
    "@types/unist": "^3.0.2",
    "mdast-util-mdx-jsx": "^3.1.2",
    "mdast-util-mdxjs-esm": "^2.0.1",
    "prettier": "^3.3.3",
    "unist-util-flatmap": "^1.0.0",
    "unist-util-visit": "^5.0.0",
    "vfile": "^6.0.2"
  },
  "packageManager": "yarn@4.4.0",
  "peerDependencies": {
    "typescript": "*"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "sideEffects": false
}
