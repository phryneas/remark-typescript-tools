{
  "compilerOptions": {
    "rootDir": ".",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "allowUnusedLabels": true,
    "skipLibCheck": true,
    "noEmitOnError": false,
    "strict": true,
    "removeComments": false,
    "module": "ESNext",
    "target": "ESNext",
    "alwaysStrict": false,
    "baseUrl": ".",
    "paths": {
      "@transpileCodeblocksTest": ["test/transpileCodeblocks.test.ts"],
      "@test/*": ["test/*"]
    }
  }
}
